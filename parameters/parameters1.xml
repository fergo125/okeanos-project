<template>
  <datasource>test.nc</datasource>
  <title fontsize="15">Prueba de Oleaje</title>
  <variables>
    <time>time</time>
    <var type="normal">Significant_height_of_combined_wind_waves_and_swell_surface</var>
    <lat>lat</lat>
    <lon>lon</lon>
  </variables>
  <layers max_lat="30" max_lon="300" min_lat="0" min_lon="260" background_color="" map_color="">
    <layer var_name="Significant_height_of_combined_wind_waves_and_swell_surface" type="colormesh">
      <params position='bottom' vmax='4' vmin='0'></params>
    </layer>
    <layer var_name="Significant_height_of_combined_wind_waves_and_swell_surface" type="contour">
      <params color='k' linewidths='0.5'></params>
    </layer>
  </layers>
  <output type='collection'>test_params_collection</output>
</template>
