<template>
  <title fontsize="15">Prueba de Viento</title>
  <variables_dataset datasource="Data/test_wind.nc">
    <var level="0" output_name="u-component-wind">u-component_of_wind_height_above_ground</var>
    <var level="0" output_name="v-component-wind">v-component_of_wind_height_above_ground</var>
    <time>time1</time>
    <lat>lat</lat>
    <lon>lon</lon>
  </variables_dataset>
  <variables_template>
    <var value_u="u-component-wind" value_v="u-component-wind" type="magnitude">wind_magnitude</var>
    <var value_u="u-component-wind" value_v="u-component-wind" type="vector">wind_vectors</var>
  </variables_template>
  <layers max_lat="30" max_lon="290" min_lat="0" min_lon="260" background_color="" map_color="">
    <layer var_name="wind_magnitude" type="colormesh">
      <params position="bottom"></params>
    </layer>
    <layer var_name="wind_magnitude" type="contour">
      <params color='k' linewidths='0.5'></params>
    </layer>
    <layer var_name="wind_vectors" type="arrows"><params></params></layer>
  </layers>
  <output type='collection'>test_wind_collection</output>
</template>
